  "name": "Stella"
  "description": "TG grp manager bot based on pyrogram",
  "keywords": [
         "telegram",
         "anime",
         "group",
         "manager",
         "Stella"
      ],   
   "repository": "https://github.com/satyanandatripathi/Stella",

  "env": {
    "OWNER_ID": {
      "description": "Telegram UID of owner account",
      "required": true,
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true,
    },
    "BOT_USERNAME": {
      "description": "Your bot's username without @",
      "required": true,
    },
    "BOT_ID": {
      "description": "Telegram UID of your bot",
      "required": true,
    },
    "PREFIX": {
      "description": "command handler of your bot",
      "required": false,
      "value": "/"
    },
    "DATABASE_URI": {
      "description": "MongoDB uri",
      "required": true,
    },
    "BACKUP_CHAT": {
      "description": "Chat id of backup chat",
      "required": true
    },
    "LOG_CHANNEL": {
      "description": "Chat id of log channel of ur bot",
      "required": true,
    },    
    "SUDO_USERS": {
      "description": "Telegram UID of users you wanna gib sudo access to your bot (seperate by space )",
      "required": false,   
    
    }
  }
}
